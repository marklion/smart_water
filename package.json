    "engines": {
        "node": "^20.19.0 || >=22.12.0"
    },
    "scripts": {
        "start_server": "cross-env NODE_ENV=development PORT=47147 node ./public/server/index.js",
        "dev_server": "cross-env NODE_ENV=development PORT=47147 nodemon --watch ./ --exec babel-node ./public/server/index.js",
        "dev_test_cli": "nodemon --no-restartable -I --quiet --watch ./ --exec babel-node ./public/cli/index.js",
        "dev_cli": "node --trace-warnings ./public/cli/index.js",
        "dev_gui": "vite --config public/gui/vite.config.js",
        "build": "rm -rf dist && npm run build_mobile && vite build --config public/gui/vite.config.js && rollup -c rollup_server.config.js && rollup -c rollup_cli.config.js",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest --detectOpenHandles --runInBand",
        "citest": "NODE_OPTIONS=--experimental-vm-modules CI=true jest --config jest.ci.config.js --detectOpenHandles --runInBand --verbose",
        "dev_mobile": "cross-env UNI_INPUT_DIR=public/mobile/src VITE_ROOT_DIR=public/mobile uni",
        "build_mobile": "cross-env UNI_INPUT_DIR=public/mobile/src VITE_ROOT_DIR=public/mobile uni build --outDir ../server/mobile",
        "build_wx_mp": "cross-env UNI_INPUT_DIR=public/mobile/src VITE_ROOT_DIR=public/mobile uni -p mp-weixin"
    },
